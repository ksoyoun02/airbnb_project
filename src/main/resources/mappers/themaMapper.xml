<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="ThemaMapper">
  
  	<select id="cultureList" resultType="com.spring.airbnb.domain.ThemaDTO">
		SELECT *
		FROM thema
		WHERE tag = 'C'
	</select>
	
	<select id="activityList" resultType="com.spring.airbnb.domain.ThemaDTO">
		SELECT *
		FROM thema
		WHERE tag = 'A'
	</select>
	
	<select id="healingList" resultType="com.spring.airbnb.domain.ThemaDTO">
		SELECT *
		FROM thema
		WHERE tag = 'H'
	</select>
  </mapper>